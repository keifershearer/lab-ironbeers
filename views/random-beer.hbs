<h1>Random Beer</h1>
<section>
    {{#with randomBeer}}
    <img src="{{image_url}}" alt="image">
    <h1>{{name}}</h1>
    <p>{{description}}</p>
    <p>{{tagline}}</p>
    <ul>
        
        {{#each food_pairing}}
            <li>{{this}}</li>

        {{/each}}
    </ul>
    <p>{{brewers_tips}}</p>
    {{/with}}
</section>